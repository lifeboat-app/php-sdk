{
    "name": "lifeboat-app/php-sdk",
    "description": "Lifeboat PHP SDK",
    "license": "BSD-3-Clause",
    "autoload": {
        "psr-4": {
            "Lifeboat\\SDK\\": "src",
            "Lifeboat\\SDK\\Services\\": "src/services",
            "Lifeboat\\SDK\\Exceptions\\": "src/exceptions"
        }
    },
    "require": {
        "php": ">=7.2.0",
        "ext-json": "*",
        "ext-zip": "*",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "guzzlehttp/promises": ">=1.4"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
