{
    "name": "lifeboat/php-sdk",
    "type": "library",
    "description": "Lifeboat PHP SDK",
    "license": "BSD-3-Clause",
    "homepage": "https://lifeboat.app",
    "keywords": [
        "lifeboat", "sdk"
    ],
    "authors": [
        {
            "name": "Lifeboat",
            "email": "hello@lifeboat.app",
            "homepage": "https://lifeboat.app",
            "role": "Developer"
        },
        {
            "name": "Lifeboat Community",
            "homepage": "https://lifeboat.app",
            "role": "Mantainer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Lifeboat\\": "src",
            "Lifeboat\\Services\\": "src/services",
            "Lifeboat\\Exceptions\\": "src/exceptions",
            "Lifeboat\\Utils\\": "src/utils",
            "Lifeboat\\Traits\\": "src/traits",
            "Lifeboat\\Interfaces\\": "src/interfaces",
            "Lifeboat\\Resource\\": "src/resource",
            "Lifeboat\\Models\\": "src/models",
            "Lifeboat\\Lists\\": "src/lists"
        }
    },
    "require": {
        "php": ">=7.4.0",
        "ext-json": "*",
        "ext-curl": "*",
        "guzzlehttp/promises": ">=1.5"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
